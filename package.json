{
  "name": "pie-fly-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build:local": "nest build && copy .env dist && copy package.json dist && mkdir dist\\assets && copy src\\assets\\* dist\\assets\\",
    "build:dev": "nest build && copy .env_dev dist\\.env && copy package.json dist && mkdir dist\\assets && copy src\\assets\\* dist\\assets\\ && mkdir dist\\templates && copy src\\templates\\* dist\\templates\\",
    "build:qat": "nest build && cp .env_qat dist/.env && cp package.json dist/ && cp -r src/assets dist/ && cp -r src/templates dist/",
    "build": "nest build && cp .env dist/ && cp package.json dist/ && cp -r src/assets dist/ && cp -r src/templates dist/",
    "g": "npx nest g resource",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "prod": "node main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pre-mig:create": "cd ./src/pre-migrations/ && typeorm-ts-node-esm migration:create",
    "post-mig:create": "cd ./src/post-migrations/ && typeorm-ts-node-esm migration:create"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/typeorm": "^10.0.2",
    "app-info-parser": "^1.1.6",
    "aws-sdk": "^2.1691.0",
    "axios": "^1.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "firebase-admin": "^13.0.1",
    "geolib": "^3.3.4",
    "handlebars": "^4.7.8",
    "jsonwebtoken": "^9.0.2",
    "kafkajs": "^2.2.4",
    "ldapjs": "^3.0.7",
    "nodemailer": "^6.9.14",
    "pg": "^8.12.0",
    "puppeteer": "^23.10.4",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "streamifier": "^0.1.1",
    "typeorm": "^0.3.20",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/platform-express": "^10.4.6",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.2",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.3.1",
    "@types/node-cron": "^3.0.11",
    "@types/puppeteer": "^5.4.7",
    "@types/streamifier": "^0.1.2",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "node-cron": "^3.0.3",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
